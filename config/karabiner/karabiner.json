{
  "description": "Keys launching applications",
  "manipulators": [
    {
      "from": {
        "key_code": "t",
        "modifiers": {
          "mandatory": ["command", "control"],
          "optional": ["any"]
        }
      },
      "to": [{ "shell_command": "open -a 'Ghostty.app'" }],
      "type": "basic"
    }
  ]
}